{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "numericSeparator": false,
      "classPrivateProperty": false,
      "privateMethod": false,
      "classProperty": false,
      "functionBind": false,
      "decorators": false,
      "decoratorsBeforeExport": false
    },
    "externalHelpers": true,
    "loose": false,
    "experimental": {
      "plugins": [
        [
          "@swc/plugin-styled-components",
          {
            "displayName": true,
            "ssr": false
          }
        ]
      ]
    }
  }
}
