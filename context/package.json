    "build:analyze": "webpack --progress --config build-utils/webpack.analyze.js",
    "build:maintain": "webpack --config build-utils/webpack.maintenance.js",
    "dev-server": "webpack-dev-server --config build-utils/webpack.dev.js --hot",
    "dev-server:maintain": "webpack-dev-server --config build-utils/webpack.dev.maintenance.js --hot",
