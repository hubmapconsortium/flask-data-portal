{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "sourceMap": true,
    "noEmit": true,
    "paths": {
      "js/*": [
        "./app/static/js/*"
      ],
      "@mui/styled-engine": [
        "./node_modules/@mui/styled-engine-sc"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "**/*.bundle.js",
    "@storybook/types/dist"
  ],
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}